<% content_for(:title) do%>
	Special Scheduling Settings
<% end %>
<h1>Special Scheduling Settings</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
  </tr>

<% @special_schedule_settings.each do |special_schedule_setting| %>
  <tr>
    <td><%=h special_schedule_setting.name %></td>
    <td><%=h special_schedule_setting.value %></td>
    <td><%= link_to 'Show', special_schedule_setting %></td>
    <td><%= link_to 'Edit', edit_special_schedule_setting_path(special_schedule_setting) %></td>
    <td><%= link_to 'Destroy', special_schedule_setting, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Setting', new_special_schedule_setting_path %>