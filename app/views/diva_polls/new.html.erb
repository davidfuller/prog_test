<h1>New diva_poll</h1>

<% form_for(@diva_poll) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :media_search %><br />
    <%= f.text_field :media_search %>
  </p>
  <p>
    <%= f.label :workflow_start %><br />
    <%= f.text_field :workflow_start %>
  </p>
  <p>
    <%= f.label :workflow %><br />
    <%= f.text_field :workflow %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', diva_polls_path %>