<h1>On Demand Scheduling Messages</h1>

<table>
  <tr>
    <th>Message</th>
    <th>Default</th>
  </tr>

<% @on_demand_schedulings.each do |on_demand_scheduling| %>
  <tr>
    <td><%=h on_demand_scheduling.message %></td>
    <td><%=h on_demand_scheduling.default %></td>
    <td><%= link_to 'Show', on_demand_scheduling %></td>
    <td><%= link_to 'Edit', edit_on_demand_scheduling_path(on_demand_scheduling) %></td>
    <td><%= link_to 'Destroy', on_demand_scheduling, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Message', new_on_demand_scheduling_path %>
