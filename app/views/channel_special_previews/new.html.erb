<h1>New channel_special_preview</h1>

<% form_for(@channel_special_preview) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :channel_id %><br />
    <%= f.text_field :channel_id %>
  </p>
  <p>
    <%= f.label :special_preview_id %><br />
    <%= f.text_field :special_preview_id %>
  </p>
  <p>
    <%= f.label :enable %><br />
    <%= f.check_box :enable %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', channel_special_previews_path %>