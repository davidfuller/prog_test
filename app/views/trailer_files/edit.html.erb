<h1>Editing trailer_file</h1>

<% form_for(@trailer_file) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :filename %><br />
    <%= f.text_field :filename %>
  </p>
  <p>
    <%= f.label :uploaded %><br />
    <%= f.datetime_select :uploaded %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @trailer_file %> |
<%= link_to 'Back', trailer_files_path %>