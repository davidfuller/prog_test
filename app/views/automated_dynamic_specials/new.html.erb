<% content_for(:title) do%>
	New Automated Dynamic Special: MuVi2 Database
<% end %>
<h1>New Automated Dynamic Special</h1>

<% form_for @automated_dynamic_special, :url => automated_dynamic_specials_path(:index_params => @index_params), :method => :post do |f| %>
  <%= f.error_messages %>

  <%= render :partial => 'entry', :locals => {:f => f} %>
  
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= render :partial => 'demo_wrapper', :locals => {:button => 'Create'} %>

<%= link_to 'Back', automated_dynamic_specials_path %>
